        "class Personne:\n",
        "    def __init__(self, nm, pm, ag, lieu):  # Méthode constructeur\n",
        "        self.nom = nm\n",
        "        self.prenom = pm\n",
        "        self.age = ag\n",
        "        self.ville = lieu\n"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "# Création d'une instance de la classe Personne\n",
        "p1 = Personne(\"Safa\", \"Jomni\", 23, \"tunis\")\n",
        "\n",
        "# Accès aux attributs\n",
        "print(p1.nom)  # Safa\n",
        "print(p1.prenom)  # Jomni\n",
